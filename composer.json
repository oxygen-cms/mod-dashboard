{
    "name": "oxygen/mod-dashboard",
    "description": "Oxygen Dashboard",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "oxygen/core": "~0.6.0",
        "illuminate/support": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OxygenModule\\Dashboard\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen Module - Dashboard",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Dashboard",
                "class": "OxygenModule\\Dashboard\\DashboardServiceProvider",
                "description": "Registers the Dashboard"
            }
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18"
    }
}
